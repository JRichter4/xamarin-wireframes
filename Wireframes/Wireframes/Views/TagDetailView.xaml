<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Wireframes.Views.TagDetailView">
    
    <ContentView.Content>
        <StackLayout>
            <Label Text="Tag Name" FontAttributes="Bold"/>
            <Entry Text="{Binding Path=Tag.Name}"/>
        </StackLayout>
    </ContentView.Content>
    
</ContentView>