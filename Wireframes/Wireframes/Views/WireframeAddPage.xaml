<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Wireframes.Views.WireframeAddPage"
             xmlns:localViews="clr-namespace:Wireframes.Views"
             Title="Add Wireframe">
    
    <ContentPage.Content>
        <StackLayout>
            <localViews:WireframeDetailView/>
            <Button Text="Save" Margin="10,0,10,0" BackgroundColor="#4BD865"
                    Command="{Binding SaveWireframeCommand}"/>
        </StackLayout>
    </ContentPage.Content>
    
</ContentPage>